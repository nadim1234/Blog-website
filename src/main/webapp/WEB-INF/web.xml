<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  <servlet-name>allarticles</servlet-name>
  <servlet-class>com.articles.AllArticles</servlet-class>
  </servlet>
  
   <servlet>
  <servlet-name>newarticle</servlet-name>
  <servlet-class>com.articles.NewArticleController</servlet-class>
  </servlet>
  
    <servlet>
  <servlet-name>readMore</servlet-name>
  <servlet-class>com.articles.ReadMoreController</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>savearticle</servlet-name>
  <servlet-class>com.articles.SaveArticleController</servlet-class>
  </servlet>
  
    <servlet>
  <servlet-name>editarticle</servlet-name>
  <servlet-class>com.articles.EditController</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>deletearticle</servlet-name>
  <servlet-class>com.articles.DeleteController</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  <servlet-name>allarticles</servlet-name>
  <url-pattern>/all</url-pattern>
  </servlet-mapping>
  

 <servlet-mapping>
  <servlet-name>newarticle</servlet-name>
  <url-pattern>/new</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>savearticle</servlet-name>
  <url-pattern>/save</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>readMore</servlet-name>
  <url-pattern>/readMore</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>editarticle</servlet-name>
  <url-pattern>/edit</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
  <servlet-name>deletearticle</servlet-name>
  <url-pattern>/delete</url-pattern>
  </servlet-mapping>
 
 
  
</web-app>
